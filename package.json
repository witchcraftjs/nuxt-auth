{
	"name": "@witchcraft/nuxt-auth",
	"version": "0.0.0",
	"description": "Nuxt module for logging.",
	"repository": "witchcraftjs/nuxt-auth",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"//exports": {
		"require in ./*": "Temporary for drizzle."
	},
	"exports": {
		".": {
			"types": "./dist/types.d.mts",
			"import": "./dist/module.mjs"
		},
		"./*": {
			"types": "./dist/runtime/*.d.ts",
			"import": "./dist/runtime/*",
			"require": "./dist/runtime/*"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "husky && cd playground && pnpm i",
		"build": "cross-env-shell DISABLE_POSTGRES_ENSURE_ENV=true 'nuxt-module-build prepare && nuxt-module-build build && pnpm nuxi build playground'",
		"//": "drizzle requires we run from playground",
		"dev:playground": "cd playground && pnpm dev",
		"dev:prepare": "cross-env-shell DISABLE_POSTGRES_ENSURE_ENV=true 'pnpm nuxt-module-build build --stub && pnpm nuxt-module-build prepare && pnpm nuxi prepare playground'",
		"lint": "eslint \"{src,test,playground/app}/**/*.{ts,vue}\" \"*.{js,cjs,mjs,ts}\"",
		"lint:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest watch"
	},
	"peerDependencies": {
		"drizzle-orm": "^0.44.3"
	},
	"@comments": {
		"dependencies": {
			"@witchcraft/*": "TODO remove and properly install before publishing."
		}
	},
	"dependencies": {
		"@alanscodelog/tsconfigs": "^6.0.0",
		"@alanscodelog/utils": "^5.1.0",
		"@nuxt/kit": "^4.0.1",
		"@vueuse/core": "^13.1.0",
		"@witchcraft/nuxt-logger": "link:../nuxt-logger",
		"@witchcraft/nuxt-postgres": "link:../nuxt-utils",
		"@witchcraft/nuxt-utils": "^0.1.0",
		"@witchcraft/ui": "~0.1.0",
		"arctic": "~3.0.0",
		"cookie-es": "^2.0.0",
		"defu": "latest",
		"jsonwebtoken": "^9.0.2",
		"ms": "^2.1.3",
		"nuxt-security": "^2.3.0",
		"pino": "^9.4.0",
		"pino-pretty": "^13.0.0",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^28.5.0",
		"zod": "^4.0.9"
	},
	"devDependencies": {
		"@alanscodelog/eslint-config": "^5.0.0",
		"@iconify/json": "^2.2.254",
		"@nuxt/devtools": "^2.4.0",
		"@nuxt/eslint-config": "^1.7.1",
		"@nuxt/module-builder": "^1.0.1",
		"@nuxt/schema": "^4.0.1",
		"@nuxt/test-utils": "^3.14.2",
		"@tailwindcss/cli": "^4.1.4",
		"@types/node": "latest",
		"changelogen": "^0.6.1",
		"cross-env": "^7.0.3",
		"drizzle-orm": "^0.44.3",
		"electron": "^34.0.0",
		"eslint": "^9.25.0",
		"husky": "^9.1.7",
		"nuxt": "^4.0.2",
		"tailwindcss": "^4.1.4",
		"typescript": "~5.8.3",
		"vitest": "^3.1.1",
		"vue-tsc": "^3.0.4"
	}
}
